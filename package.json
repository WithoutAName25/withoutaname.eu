{
  "name": "withoutaname",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vue-tsc --noEmit && vite build && vite preview",
    "test": "jest --coverage --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.(ts|vue)"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "vue"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.vue$": "@vue/vue3-jest",
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.3.0-beta3",
    "@fortawesome/free-brands-svg-icons": "^6.0.0-beta3",
    "@fortawesome/free-regular-svg-icons": "^6.0.0-beta3",
    "@fortawesome/free-solid-svg-icons": "^6.0.0-beta3",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "vue": "^3.2.16",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@vitejs/plugin-legacy": "^1.6.2",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/test-utils": "^2.0.0-rc.17",
    "@vue/vue3-jest": "^27.0.0-alpha.3",
    "jest": "^27.3.1",
    "jest-each": "^27.4.6",
    "sass": "^1.43.3",
    "sass-loader": "^12.2.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.3",
    "vite": "^2.6.13",
    "vue-tsc": "^0.31.1"
  }
}
