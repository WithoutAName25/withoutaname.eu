{
  "private": true,
  "packageManager": "pnpm@6.32.3",
  "scripts": {
    "build": "vue-tsc --noEmit && vite-ssg build",
    "dev": "vite --port 3333",
    "dev:instrumented": "env VITE_COVERAGE=true vite --port 3333",
    "format": "prettier --write .",
    "preview": "vite preview --port 3333",
    "preview:ci": "pnpm run build && pnpm run preview",
    "preview-https": "serve dist",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "cy": "cypress open",
    "cy:run": "cypress run",
    "cy:ci": "start-server-and-test dev:instrumented http-get://127.0.0.1:3333 cy:run"
  },
  "dependencies": {
    "@vueuse/components": "^8.4.2",
    "@vueuse/core": "^8.4.2",
    "@vueuse/head": "^0.7.6",
    "file-saver": "^2.0.5",
    "nprogress": "^0.2.0",
    "open-props": "^1.3.16",
    "pinia": "^2.0.14",
    "vue": "^3.2.33",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.12",
    "@iconify/vue": "^3.2.1",
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@types/file-saver": "^2.0.5",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/node": "^17.0.31",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-legacy": "^1.8.2",
    "@vitejs/plugin-vue": "^2.3.2",
    "@vue/test-utils": "^2.0.0-rc.21",
    "autoprefixer": "^10.4.7",
    "c8": "^7.11.2",
    "cross-env": "^7.0.3",
    "cypress": "^9.6.0",
    "https-localhost": "^4.7.1",
    "markdown-it-link-attributes": "^4.0.0",
    "postcss": "^8.4.13",
    "postcss-custom-media": "^8.0.0",
    "postcss-jit-props": "^1.0.6",
    "postcss-nesting": "^10.1.4",
    "postcss-preset-env": "^7.5.0",
    "prettier": "^2.6.2",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.7.1",
    "vite": "^2.9.8",
    "vite-plugin-istanbul": "^2.7.3",
    "vite-plugin-md": "^0.13.0",
    "vite-plugin-pages": "^0.23.0",
    "vite-plugin-pwa": "^0.12.0",
    "vite-ssg": "^0.20.0",
    "vite-ssg-sitemap": "^0.2.6",
    "vitest": "^0.10.5",
    "vue-tsc": "^0.34.11"
  }
}
